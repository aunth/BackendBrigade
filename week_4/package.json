    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "mongoose": "^8.2.2",
    "ncp": "^2.0.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "start-server": "^0.0.4",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/express-session": "^1.18.0",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14"
  },
